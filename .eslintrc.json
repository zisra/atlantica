{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"no-console": "error"
	}
}
